{
  "buildCommand": "cd decisivis-dashboard && npm run build",
  "installCommand": "cd decisivis-dashboard && npm install --legacy-peer-deps",
  "devCommand": "cd decisivis-dashboard && npm run dev",
  "outputDirectory": "decisivis-dashboard/.next",
  "framework": "nextjs",
  "env": {
    "GEMINI_TEMPERATURE": "0.1",
    "GEMINI_MODEL": "gemini-pro",
    "NODE_ENV": "production"
  }
}